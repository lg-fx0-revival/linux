};

&sdhc_1 {
	status = "okay";

	vmmc-supply = <&pm8226_l17>;
	vqmmc-supply = <&pm8226_l6>;
};

&sdhc_2 {
	status = "okay";
	
	vmmc-supply = <&pm8226_l18>;
	vqmmc-supply = <&pm8226_l21>;
};

&usb {
	status = "okay";
	extcon = <&smbb>;
	dr_mode = "peripheral";
};

&usb_hs_phy {
	extcon = <&smbb>;
	v1p8-supply = <&pm8226_l10>;
	v3p3-supply = <&pm8226_l20>;
};


&tlmm {
	gpio_keys_default: gpio-keys-active {
		pinmux {
			function = "gpio";
			pins = "gpio106", "gpio107", "gpio108";
		};

		pinconf {
			pins = "gpio106", "gpio107", "gpio108";
			drive-strength = <2>;
			bias-pull-up;
		};
	};
};